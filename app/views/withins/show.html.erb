
<!-- divider -->                
<div class="row">
    <table class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>#</th>
                <th>Clan</th>
                <th>Tribe</th>
                <th>#</th>
            </tr>
        </thead>
        <tbody>
            <% @clan.tribe.each_with_rel do |_n, r| %>
              <tr>
                  <td>1</td>
                  <td><%= r.from_node[:name] %></td>
                  <td><%= r.to_node[:name] %> </td>
                  <td><%= link_to("delete",clan_within_path(r), method: :delete) %> </td>
              </tr>
            <% end %>
        </tbody>
    </table> 
</div>
<!-- end divider -->